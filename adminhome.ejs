            <div class="bg-white h-100 p-4 shadow">
              <h3 class="mb-4">List of NGOs</h3>
              <div class="table-responsive">
                <table class="table table-striped table-bordered table-responsive-xl fixed_header">
                <tr style="background-color: #343a40; color: white; border-color: #454d55;">
                  <th>NAME</th><th>CONTACT</th><th>ADDRESS</th><th>EMAIL</th>
                </tr>
                  <% for (var d=0;d<doc1.length;d++){%>
                    <tr>
                        <td><%= doc1[d].nname %></td><td><%= doc1[d].contact %></td><td><%= doc1[d].address %></td><td><%= doc1[d].email %></td>
                    </tr>
                  <% } %>
