        <div class="row align-items-stretch">
        <div class="col-md-12">
          <div class="bg-white h-100 p-4 shadow">
            <h3 class="mb-4">List of Donators</h3>
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-responsive-xl fixed_header">
                <tr style="background-color: #343a40; color: white; border-color: #454d55;">
                  <th>NAME</th><th>CONTACT NO</th><th>MEDICINE</th><th>DATE</th>
                </tr>
                <% for (var d=0;d<doc2.length;d++){%>
                <tr>
                    <td><%= doc2[d].name %></td><td><%= doc2[d].contact %></td><td><%= doc2[d].medicine %></td><td><%= doc2[d].myDate %></td>
                </tr>      
              <% } %>
